def stickman(x,y):
  t.penup()
  t.goto(x,y)
  t.pendown()
  t.color("cornsilk")
  t.circle(10)
  t.setheading(270)
  t.forward(15)
  t.left(120)
  t.forward(20)
  t.backward(20)
  t.left(60)
  t.right(120)
  t.forward(20)
  t.backward(20)
  t.right(60)
  t.forward(10)
  t.left(30)
  t.forward(20)
  t.backward(20)
  t.left(60)
  t.forward(20)
  t.backward(10)
  t.right(150)
stickman(65,130)

def stickman2(x,y):
  t.penup()
  t.goto(x,y)
  t.pendown()
  t.color("white")
  t.setheading(0)
  t.circle(10)
  t.setheading(270)
  t.forward(20)
  t.left(60)
  t.forward(20)
  t.backward(20)
  t.right(120)
  t.forward(20)
  t.backward(20)
  t.left(60)
  t.forward(20)
  t.left(40)
  t.forward(20)
  t.backward(20)
  t.right(80)
  t.forward(20)
stickman2(-40,100)
stickman2(-10,140)
stickman2(-50,120)
stickman2(-80,140)
