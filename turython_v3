def filled_circle(radius):
    t.color("royalblue")
    t.begin_fill()
    t.circle(radius)
    t.end_fill()
def cloud(radius,x,y):
    t.penup()
    t.goto(x,y)
    t.pendown()
    t.setheading(60)
    filled_circle(radius)
    t.forward(radius)
    filled_circle(radius)
    t.right(90)
    filled_circle(radius)
    t.right(90)
    filled_circle(radius)
    t.right(90)
    filled_circle(radius)
    t.right(90)

cloud(40,-40,80)
cloud(40,40,80)
t.pencolor("white")
t.penup()
t.goto(10,53)
t.pendown()
t.left(30)
t.forward(135)
def paralelogram(x,y):
  t.penup()
  t.goto(x,y)
  t.pendown()
  t.setheading(0)
  t.color("saddlebrown")
  t.begin_fill()
  for i in range(2):
   t.forward(20)
   t.right(30)
   t.forward(40)
   t.right(150)
  t.end_fill()
paralelogram(60,130)
t.fillcolor("saddle brown")
t.begin_fill()
t.right(90)
t.forward(20)
