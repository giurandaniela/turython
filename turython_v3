#flori
def flower(x,y,c,l,cc):
  t.setheading(0)
  t.penup()
  t.goto(x,y)
  t.pendown()
  t.color(cc)
  t.begin_fill()
  for i in range(2):
    t.forward(2)
    t.left(90)
    t.forward(20)
    t.left(90)
  t.penup()
  t.goto(x,y)
  t.pendown()
  t.circle(30,70)
  t.left(110)
  t.circle(30,70)
  t.penup()
  t.goto(x,y)
  t.pendown()
  t.setheading(280)
  t.circle(-30,-70)
  t.left(110)
  t.circle(-30,-70)
  t.end_fill()
  
  
  t.color(c)
  t.penup()
  t.goto(x+11,y+23)
  t.pendown()
  for i in range(l):
    t.forward(20)
    t.right(165)


flower(-150, 80,"red",12,"green")
flower(-150, 150,"red",12,"green")
flower(165, 80,"peru",11,"darkgoldenrod")
flower(150, 150,"peru",11,"darkgoldenrod")

#eye
def eye(x,y):
  t.penup()
  t.goto(x,y)
  t.pendown()
  t.setheading(170)
  t.color("white")
  t.begin_fill()
  t.circle(25,40)
  t.left(110)
  t.circle(25,70)
  t.end_fill()
  t.penup()
  t.goto(x,y-7)
  t.pendown()
  t.color("blue")
  t.begin_fill()
  t.circle(3)
  t.end_fill()
  t.penup()
  t.goto(x-1,y-4)
  t.pendown()
  t.color("black")
  t.begin_fill()
  t.circle(0.5)
  t.end_fill()
eye(30,10)
eye(-10,10)
t.penup()
t.goto(5,3)
  
def mouth(x,y):
  t.penup()
  t.goto(x,y)
  t.pendown()
  t.setheading(110)
  t.color("red")
  t.begin_fill()
  t.circle(10,70)
  t.left(110)
  t.circle(10,70)
  t.circle(10,70)
  t.left(110)
  t.circle(10,70)
  t.penup()
  t.goto(x+1,y+2)
  t.pendown()
  t.setheading(0)
  for i in range(2):
    t.forward(3)
    t.left(90)
    t.forward(3)
    t.left(90)
  t.setheading(90)
  for i in range(2):
    t.forward(3)
    t.left(90)
    t.forward(6)
    t.left(90)
  t.end_fill()
mouth(10,-30)
t.pencolor("black")
t.penup()
t.goto(-30,30)
t.pendown()


def draw(rad):
  t.penup()
  t.goto(-35,35)
  t.pendown()
  t.seth(-35)
  t.color("black")
  t.begin_fill()
  t.circle(75,70)
  t.left(110)
  t.circle(75,70)
  t.end_fill()

draw(35)
